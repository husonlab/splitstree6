BEGIN ALGORITHM;
TITLE 'Show Trees';
LINK TREES = 'Working Trees';
NAME 'Show Trees';
OPTIONS
	View = 'TreeView';
END; [ALGORITHM]

BEGIN VIEW;
TITLE 'TreeView';
LINK ALGORITHM = 'Show Trees';
NAME 'TreeViewView';
INPUT 'TREES';
OPTIONS
	Tree = 1,
	Diagram = 'RectangularPhylogram',
	Averaging = 'ChildAverage',
	Orientation = 'Rotate0Deg',
	HorizontalZoomFactor = 1,
	VerticalZoomFactor = 1,
	FontScaleFactor = 1,
	LabelEdgesBy = 'None',
	ShowQRCode = 'false';
END; [VIEW]
