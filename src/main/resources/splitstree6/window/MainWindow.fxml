                                <KeyCodeCombination alt="UP" code="C" control="UP" meta="UP" shift="DOWN"
                                                    shortcut="DOWN"/>
                            </accelerator>
                        </MenuItem>
                        <MenuItem fx:id="pasteMenuItem" text="Paste"
