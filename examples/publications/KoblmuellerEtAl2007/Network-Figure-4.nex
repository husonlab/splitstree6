	20 'L.meleagris',
;
[TREES]
		[1] tree 'tree-1'=[&R] ((2,3,((5,(11)#H1),((12,#H1),(13)#H2),(6,(14,(16)#H3)),(7,8,9,10,(((17,(19)#H4),#H2),15,((#H3,#H4),(18,(20)#H5))),(#H5)),4)),1);
