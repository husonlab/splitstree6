                            <accelerator>
                                <KeyCodeCombination alt="UP" code="A" control="UP" meta="UP" shift="DOWN"
                                                    shortcut="DOWN"/>
                            </accelerator>
                        </MenuItem>
                        <MenuItem fx:id="selectInverseMenuItem" text="Select Inverse"
