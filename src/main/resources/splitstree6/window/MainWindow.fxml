                            <accelerator>
                                <KeyCodeCombination alt="UP" code="X" control="UP" meta="UP" shift="UP"
                                                    shortcut="DOWN"/>
                            </accelerator>
                        </MenuItem>
                        <MenuItem fx:id="copyMenuItem" text="Copy"
                                  userData="Copy selected text in a text tab or nodes in a workflow tab">
                            <accelerator>
                                <KeyCodeCombination alt="UP" code="C" control="UP" meta="UP" shift="UP"
                                                    shortcut="DOWN"/>
                            </accelerator>
                        </MenuItem>
                        <MenuItem fx:id="copyNewickMenuItem" text="Copy Newick"
                                  userData="Copy tree(s) and networks(s) from the current tab to the system clipboard">
                        </MenuItem>

                        <MenuItem fx:id="copyImageMenuItem" text="Copy Image"
                                  userData="Copy an image of the current tab to the system clipboard">
