                                <KeyCodeCombination alt="UP" code="ADD" control="UP" meta="UP" shift="UP"
                                                    shortcut="DOWN"/>
                            </accelerator>
                        </MenuItem>
                        <MenuItem fx:id="decreaseFontSizeMenuItem" text="Decrease Font Size"
                                  userData="Decrease the font size in the current tab">
                            <accelerator>
                                <KeyCodeCombination alt="UP" code="MINUS" control="UP" meta="UP" shift="UP"
                                                    shortcut="DOWN"/>
                            </accelerator>
                        </MenuItem>
                        <SeparatorMenuItem mnemonicParsing="false"/>
                        <MenuItem fx:id="zoomInMenuItem" text="Zoom In"
                                  userData="Increase the scale factor in the current tab">
                            <accelerator>
                                <KeyCodeCombination alt="UP" code="DOWN" control="UP" meta="UP" shift="UP"
                                                    shortcut="DOWN"/>
                            </accelerator>
                        </MenuItem>
                        <MenuItem fx:id="zoomOutMenuItem" text="Zoom Out"
                                  userData="Decrease the scale factor in the current tab">
                            <accelerator>
                                <KeyCodeCombination alt="UP" code="UP" control="UP" meta="UP" shift="UP"
                                                    shortcut="DOWN"/>
                            </accelerator>
                        </MenuItem>
                        <SeparatorMenuItem mnemonicParsing="false"/>
                        <MenuItem fx:id="zoomInHorizontalMenuItem" text="Zoom In Horizontal"
                                  userData="Increase the horizontal scale factor in the current tab">
                            <accelerator>
                                <KeyCodeCombination alt="UP" code="DOWN" control="UP" meta="UP" shift="DOWN"
                                                    shortcut="DOWN"/>
                            </accelerator>
                        </MenuItem>
                        <MenuItem fx:id="zoomOutHorizontalMenuItem" text="Zoom Out Horizontal"
                                  userData="Decrease the horizontal scale factor in the current tab">
                            <accelerator>
                                <KeyCodeCombination alt="UP" code="UP" control="UP" meta="UP" shift="DOWN"
                                                    shortcut="DOWN"/>
                            </accelerator>
                        </MenuItem>
                        <SeparatorMenuItem mnemonicParsing="false"/>
                        <MenuItem fx:id="resetMenuItem" mnemonicParsing="false" text="Reset"
                                  userData="Reset the scale factor in the current tab">
                            <accelerator>
                                <KeyCodeCombination alt="UP" code="DIGIT0" control="UP" meta="UP" shift="UP"
                                                    shortcut="DOWN"/>
                            </accelerator>
                        </MenuItem>
                        <SeparatorMenuItem mnemonicParsing="false"/>
                        <MenuItem fx:id="rotateLeftMenuItem" text="Rotate Left"
                                  userData="Left rotate the tree or network, or selected splits, in the current tab">
                            <accelerator>
                                <KeyCodeCombination alt="UP" code="LEFT" control="UP" meta="UP" shift="UP"
                                                    shortcut="DOWN"/>
                            </accelerator>
                        </MenuItem>
                        <MenuItem fx:id="rotateRightMenuItem" text="Rotate Right"
                                  userData="Right rotate the tree or network, or selected splits, in the current tab">
                            <accelerator>
                                <KeyCodeCombination alt="UP" code="RIGHT" control="UP" meta="UP" shift="UP"
                                                    shortcut="DOWN"/>
                            </accelerator>
                        </MenuItem>
                        <MenuItem fx:id="flipMenuItem" text="Flip"
                                  userData="Flip the tree or network in the current tab">
                        </MenuItem>
                        <SeparatorMenuItem mnemonicParsing="false"/>
                        <CheckMenuItem fx:id="showScaleBarMenuItem" text="Show Scale Bar"
                                       userData="Show a scale bar in the current tab"/>
                        <CheckMenuItem fx:id="showQRCodeMenuItem" text="Show QR Code"
                                       userData="Show or hide a QR code for the tree or network in the current tab"/>
                        <SeparatorMenuItem mnemonicParsing="false"/>
                        <MenuItem fx:id="layoutLabelsMenuItem" text="Layout Labels" userData="Rerun label layout">
