BEGIN SPLITSTREE6;
DIMENSIONS nDataNodes=9 nAlgorithms=6;
PROGRAM version='SplitsTree6 (version 0.0.9-alpha, built 25 May 2022)';
WORKFLOW creationDate='1653755376628'; [Sat May 28 18:29:36 CEST 2022]
END; [SPLITSTREE6]

