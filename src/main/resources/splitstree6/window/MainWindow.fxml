                                <KeyCodeCombination alt="UP" code="L" control="UP" meta="UP" shift="UP"
                                                    shortcut="DOWN"/>
                            </accelerator>
                        </MenuItem>
                    </Menu>

                    <Menu text="Data">
                        <MenuItem fx:id="filterTaxaMenuItem" text="Filter Taxa" userData="Show the filter taxa tab"/>
                        <MenuItem fx:id="filterCharactersMenuItem" text="Filter Characters..."
