BEGIN TREES;
TITLE 'Input Trees';
LINK ALGORITHM = 'Input Data Loader';
[Number of trees: 1]
PROPERTIES partialTrees=no rooted=yes reticulated=yes;
TRANSLATE
	1 'J.ornatus',
	2 'V.moorii',
	3 'T.vittatus',
	4 'L.ornatipinnis',
	5 'unnamed',
	6 'N.calliurus',
	7 'N.caudopunctatus',
	8 'N.leloupi',
