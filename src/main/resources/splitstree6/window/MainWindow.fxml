                        <MenuItem fx:id="filterSplitsMenuItem" text="Filter Splits"
                                  userData="Show the filter splits tab"/>
                        <MenuItem fx:id="splitsSliderMenuItem" text="Splits Slider"
                                  userData="Show the splits slider (filter by weight) tab"/>
                        <SeparatorMenuItem mnemonicParsing="false"/>
                        <MenuItem fx:id="traitsMenuItem" text="Edit Traits" userData="Show the edit traits tab"/>
                    </Menu>
                    <Menu text="Distances">
                        <CheckMenuItem fx:id="pDistanceMenuItem" text="P Distances"
                                       userData="Compute distances based on the normalized number of character-state differences"/>
                        <CheckMenuItem fx:id="logDetMenuItem" text="Log Det"
                                       userData="Compute distances using the Log-Det method"/>
                        <SeparatorMenuItem mnemonicParsing="false"/>
                        <CheckMenuItem fx:id="jukesCantorMenuItem" text="Jukes Cantor"
                                       userData="Compute distances using the Jukes-Cantor model"/>
